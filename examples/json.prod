json : "{ " properties  "\n}".

properties : properties WS ";" WS property.
           | property.

property   : string ":" value.

value  : number.
       | string.
       | array.
       | json.
       | TRUE.
       | FALSE.
       | NULL.

number : INTEGER.
       | FLOAT.

string : UNICODE.
       | ASCII.

array  : "[" values "]".

values : values "," value.
       | value.
